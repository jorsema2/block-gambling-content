{
  "manifest_version": 3,
  "name": "Kick Cleaner",
  "version": "1.0",
  "description": "Removes Slots and Casino categories from the Kick website.",
  "permissions": ["scripting"],
  "host_permissions": ["https://kick.com/*"],
  "content_scripts": [
    {
      "matches": ["https://kick.com/*"],
      "js": ["content.js"]
    }
  ]
}
